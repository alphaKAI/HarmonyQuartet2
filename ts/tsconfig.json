    "./gulp/node_modules/gulp-typescript/node_modules/typescript/lib/typescript.d.ts",
    "./gulp/node_modules/gulp-typescript/node_modules/typescript/lib/typescriptServices.d.ts",
    "./gulp/node_modules/typescript/lib/lib.core.d.ts",
